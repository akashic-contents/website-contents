{
	"width": 600,
	"height": 600,
	"fps": 30,
	"main": "./script/main.js",
	"assets": {
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250,
			"hint": {
				"extensions": [
					".aac",
					".ogg"
				]
			}
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"GameScene": {
			"type": "script",
			"path": "script/GameScene.js",
			"global": true
		},
		"handsign_fist": {
			"type": "image",
			"path": "image/handsign_fist.png",
			"width": 100,
			"height": 100
		},
		"handsign_open": {
			"type": "image",
			"path": "image/handsign_open.png",
			"width": 100,
			"height": 100
		},
		"handsign_victory": {
			"type": "image",
			"path": "image/handsign_victory.png",
			"width": 100,
			"height": 100
		},
		"JankenButton": {
			"type": "script",
			"path": "script/JankenButton.js",
			"global": true
		}
	},
	"environment": {
		"sandbox-runtime": "3"
	},
	"globalScripts": [],
	"exportZipInfo": {
		"version": "1.9.18",
		"option": {
			"strip": true,
			"babel": true,
			"targetService": "none"
		}
	}
}